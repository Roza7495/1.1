#include <iostream>
#include <ctime>
#include <cstdlib>
using namespace std;
int main()
{
	int n=10, i, j, count = 1, maxcount = 0,id,aid=0;
	int a[10];
	srand(time(0));
	for (i = 0;i < n;i++)
		a[i] = rand() % 5 - 2;
	for (i = 0;i < n;i++)
		cout << a[i] << endl;
	for (i = 0;i < n;i++)
	{ 
		if (a[i] != aid) {
			for (j = 1;j < n;j++)
			{
				if (a[i] == a[j]) count++;
			}
			if (count > maxcount)
			{
				maxcount = count;
				id = i;
				aid = a[i];
			}
		}
	}
	cout <<endl<< maxcount << endl;
    cout <<"stop "<<a[id]<<endl;
	system("pause");
	return 0;
}
